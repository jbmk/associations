<%#
# To change this template, choose Tools | Templates
# and open the template in the editor.
%>

<% form_for :dump, :url => {:controller => "students", :action => "excel_import"}, :html => { :multipart => true } do |f| -%>
  <table>
    <tr>
      <td>
        <label for="dump_file">
          Select an Excel File :
        </label>
      </td>
      <td >
        <%= f.file_field :file -%>
      </td>
    </tr>
    <tr>
      <td colspan='2'>
        <%= submit_tag 'Upload' -%>
      </td>
    </tr>
  </table>
<% end -%>
